<nav class="navbar navbar-inverse navbar-lower" role="navigation">
  <div class="container">
     <div class="navbar-left" role="search">
       <ul class="breadcrumb" style="background-color:#fbfbfb;border-radius:0;margin-bottom:0;margin-top:6px;">
        <li><a href="/"  onclick="window.location.href='/';"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">RDB Connection</li>
